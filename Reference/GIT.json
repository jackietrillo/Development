{
  "name":  "GIT",
  "commands": 
  [
    {
      "command": "git status",
      "description": "Displays the directory git status."
    },
    {
      "command": "git clone theurl",
      "description": "Clones a github repository."
    },
    {
      "command": "git remote -v",
      "description": "Displays the path to the remote origin."
    },
    {
      "command": "git reset HEAD myFile.txt",
      "description": "Unstages the file."
    },
    {
      "command": "git checkout HEAD myFile.txt",
      "description": "Gets the latest committed version of the file."
    },
    {
      "command": "git add .",
      "description": "Stages all added files in the current directory."
    },
	{
      "command": "git rm .",
      "description": "Stages all removed files in the current directory."
    },
	{
      "command": "git commit .",
      "description": "Commits all changes to the local git repository."
    },
    {
      "command": "git push origin master",
      "description": "Pushes all committed changes to the remote branch."
    },
  ]
}